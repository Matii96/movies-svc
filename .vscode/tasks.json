{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start environment",
      "type": "shell",
      "command": "docker-compose",
      "args": ["-f", "docker-compose-dev.yml", "up", "-d"]
    },
    {
      "label": "Stop environment",
      "type": "shell",
      "command": "docker-compose",
      "args": ["-f", "docker-compose-dev.yml", "down"],
      "presentation": {
        "reveal": "silent"
      }
    }
  ]
}
